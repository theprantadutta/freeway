# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_NAME=freeway

# Or use a full connection string (takes precedence over individual values)
# DATABASE_URL=Host=localhost;Port=5432;Database=freeway;Username=postgres;Password=your-password

# API Keys
ADMIN_API_KEY=your-admin-api-key
OPENROUTER_API_KEY=sk-or-your-openrouter-key

# Application Settings
API_KEY_PREFIX=fw_
REQUEST_TIMEOUT_SECONDS=30
COMPLETION_TIMEOUT_SECONDS=120

# Hangfire Dashboard (optional)
HANGFIRE_USERNAME=admin
HANGFIRE_PASSWORD=your-hangfire-password

# CORS (comma-separated origins, or * for all)
ALLOWED_ORIGINS=*

# Environment
ASPNETCORE_ENVIRONMENT=Development
